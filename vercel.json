{
	"trailingSlash": true,
  "outputDirectory": "build",
	"rewrites": [
		{
			"source": "/",
			"destination": "/api/serverless.api.ts"
		},
		{
			"source": "/contract/(.*)",
			"destination": "/api/contract.api.ts"
		}
	],
	"functions": {
		"api/contract.*": {
			"includeFiles": "/solidity/**/*, /lib/**/*"
		},
		"api/token.*": {
			"includeFiles": "/solidity/**/*, /lib/**/*"
		}
	}
}
